@article{eisendHowMuchHave2024,
  title = {How {{Much Have We Learned}} about {{Consumer Research}}? {{A Meta-Meta-Analysis}}},
  shorttitle = {How {{Much Have We Learned}} about {{Consumer Research}}?},
  author = {Eisend, Martin and Pol, Gratiana and Niewiadomska, Dominika and Riley, Joseph and Wedgeworth, Rick},
